    <java-symbol type="bool" name="config_handleAudioDirectionChangesBetweenCallStateChanges" />

    <!-- Whether devices suports in-display fingerprint when screen is off -->
    <java-symbol type="bool" name="config_supportsScreenOffUdfps" />

    <!-- Horizon Light -->
    <java-symbol type="integer" name="config_AmbientPulseLightColor" />

    <!-- Battery Leds -->
    <java-symbol type="bool" name="config_intrusiveBatteryLed" />

    <!-- Hide IME space -->
    <java-symbol type="dimen" name="navigation_bar_frame_height_hide_ime" />
    <java-symbol type="dimen" name="navigation_bar_frame_height_landscape_hide_ime" />

